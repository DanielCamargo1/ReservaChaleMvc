@model IEnumerable<ChaleModel>

@{
    ViewData["title"] = "Administradores Chalés";
}

<div class="containerp-5 rounded-5">
    <div class="mb-5 text-center">
        <h1 class="text-dark"> Login Administradores Chalés </h1>
    </div>
    <form method="post" asp-controller="Home" asp-action="Administrador">
        <div class="mb-3">
            <label for="Recebedor" class="form-label text-light">Login:</label>
            <input type="text" class="form-control"  placeholder="Login:">
        </div>

        <div class="mb-3">
            <label for="Fornecedor" class="form-label text-light">Senha:</label>
            <input type="text" class="form-control" placeholder="Senha: ">
        </div>

        <div class="d-flex gap-5 mt-5">
            <button type="submit" class="btn btn-success w-50">Entrar</button>
            <a class="btn btn-primary w-50" asp-controller="Home" asp-action="Index">Voltar</a>
        </div>
    </form>
</div>